{
  "name": "chat-backend",
  "version": "1.0.0",
  "description": "Simple chat backend using Gemini API and SQLite",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "concurrently \"nodemon server.js\" \"cd frontend && npm run dev\" --names \"backend,frontend\" --prefix-colors \"blue,green\"",
    "backend": "nodemon server.js",
    "frontend": "cd frontend && npm run dev",
    "install:all": "npm install && cd frontend && npm install"
  },
  "dependencies": {
    "@google/genai": "^1.14.0",
    "@google/generative-ai": "^0.21.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "sqlite3": "^5.1.6"
  },
  "devDependencies": {
    "concurrently": "^9.2.0",
    "nodemon": "^3.0.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luisf2907/open-chat.git"
  },
  "keywords": [],
